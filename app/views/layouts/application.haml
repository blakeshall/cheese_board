!!! html
%html
%head
  %title CheeseBoard
  = stylesheet_link_tag    "application", :media => "all"
  = stylesheet_link_tag    "bootstrap.min"
  = javascript_include_tag "application"
  = csrf_meta_tags

%body
  .container
    - if notice
      %p.notice
        = notice
    - if alert
      %p.alert
        = alert

    = yield
